spring.datasource.url=jdbc:mysql://localhost:3306/apontamentos?useSSL=false&createDatabaseIfNotExist=True
spring.datasource.username=root
spring.datasource.password=dpf50001

# Configurações DDL
spring.jpa.hibernate.ddl-auto=update


